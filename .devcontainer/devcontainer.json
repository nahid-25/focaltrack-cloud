{
  "name": "FocalTrack Cloud Dev",
  "image": "mcr.microsoft.com/devcontainers/typescript-node:20",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "remoteEnv": {
    "COREPACK_ENABLE_DOWNLOAD_PROMPT": "0"
  },
  "postCreateCommand": "bash -lc 'corepack enable && corepack prepare pnpm@9 --activate'"
}


